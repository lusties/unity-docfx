{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "Assets/**/*.cs"
          ],
          "exclude": [
            "Assets/Plugins/**"
          ]
        }
      ],
      "globalNamespaceId": "Global",
      "dest": "api"
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "Unity Docfx",
      "_appName": "",
      "_appFooter": "Made by Unity-Docfx: https://github.com/lusties/unity-docfx",
      "_enableSearch": true
    },
    "content": [
      {
        "src": "",
        "files": [
          "toc.yml",
          "index.md"
        ],
        "dest": ""
      },
      {
        "src": "manual",
        "files": [
          "toc.yml",
          "*.md"
        ],
        "dest": "manual"
      },
      {
        "src": "api",
        "files": [
          "*.yml"
        ],
        "dest": "api"
      }
    ],
    "dest": "../docs",
    "template": [
      "default",
      "templates/darkfx"
    ]
  }
}